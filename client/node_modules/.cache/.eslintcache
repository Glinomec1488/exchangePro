\n                    currencies && currencies.map(currency => (\n                        <div\n                            key={currency.fullName}\n                            onClick={() => handleSelectCurrency(currency.shortName, currency.fullName)}\n                            className={`exchange__block-item exchange__block-item-send ${getActiveCurrencyStyle(isTo, currency.fullName, fromCurrency.fullName, toCurrency.fullName)}`}\n                        >\n                            <img src={`${getEnv(process.env.REACT_APP_SERVER_URL, 'REACT_APP_SERVER_URL')}/static/${currency.imageUrlP}.svg`} alt=\"\" />\n                        </div>\n                    ))\n                }\n            </div>\n        </div>\n    )\n}\n\nexport default CurrencyDropDown","/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/exchange/components/form/index.tsx",["206"],[],"/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/exchange/components/currenciesList/index.tsx",[],[],"/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/exchange/hooks/useCurrency.ts",[],[],"/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/exchange/components/form/hooks/useForm.ts",[],[],"/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/exchange/components/form/hooks/useExchangeRate.ts",["207","208","209"],[],"/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/howExchange/components/walletBonus/enum/index.ts",[],[],"/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/howExchange/components/walletBonus/helpers/index.ts",[],[],"/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/exchange/components/currenciesList/helpers/index.ts",[],[],"/home/mayaku/Downloads/test/ex-main/client/src/screens/home/components/exchange/components/form/components/captcha/index.tsx",["210"],[],"import { useEffect, useState } from \"react\"\nimport { useAppDispatch, use